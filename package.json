{
  "name": "jawbreaker-server",
  "version": "1.0.0",
  "description": "Backend server for Jawbreaker AI-Powered Clinical Insight Platform",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "start": "node index.js",
    "lint": "echo \"(optional) add eslint\"",
