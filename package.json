    "test:email": "node test-email-only.js",
    "test:real-email": "node test-real-email.js",
    "test:env": "node test-env.js",
    "test:ai-service": "node test-ai-service.js",
    "use:dev": "node switch-env.cjs dev",
    "use:prod": "node switch-env.cjs prod",
    "seed:patients": "node seed-patients.js",
    "seed:clinical-data": "node seed-clinical-data.js",
    "seed:note-templates": "node seed-note-templates.js",
    "seed:all-data": "node seed-all-data.js",
    "seed:all": "npm run seed:patients && npm run seed:clinical-data && npm run seed-note-templates",
    "test-server": "node test-server.js",
    "test:payment": "node tests/quick-test.js",
    "test:webhook": "node tests/mock-webhook-test.js",
